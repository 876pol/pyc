int main() {
    // Plays FizzBuzz for integers up to 20. Example of control structures.
    for (int i = 0; i < 20; i += 1) {
        if (i % 15 == 0) {
            print("FizzBuzz\n");
        } else if (i % 5 == 0) {
            print("Buzz\n");
        } else if (i % 3 == 0) {
            print("Fizz\n");
        } else {
            print((string) i + "\n");
        }
    }
    return 0;
}